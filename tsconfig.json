{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/assets/*": [
        "./assets/*"
      ],
      "@/global.css": [
        "./global.css"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "nativewind-env.d.ts"
  ]
}
